#include<bits/stdc++.h>
using namespace std;
using ll =long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll n;
        cin>>n;
        ll a[n];
        for(ll i=0;i<n;i++) cin>>a[i];
        bool f=true;
        ll mn=1e9;
        for(ll i=0;i<n;i++){
            if(mn<a[i]){
                if(2*mn<=a[i]){
                    f=false;
                    break;
                }
            }
            else mn=a[i];
        }
        if(f==false) cout<<"NO\n";
        else cout<<"YES\n";
    }
}
