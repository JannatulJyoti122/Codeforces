#include<bits/stdc++.h>
using namespace std;
using ll=long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        int n;
        cin>>n;
        int a[n];
        int gcd=0;
        for(int i=0;i<n;i++){
            cin>>a[i];
            gcd=__gcd(gcd,a[i]);
        }
        int ct_one=0;
        for(int i=0;i<n;i++){
            if(a[i]==gcd) ct_one++;
        }
        if(ct_one>0){
            cout<<n-ct_one<<endl;
            continue;
        }
        vector<int>dp(5001,1e9);
        queue<int>q;
        for(int i=0;i<n;i++){
            dp[a[i]]=0;
            q.push(a[i]);
        }
        while(!q.empty()){
            int p=q.front();
            q.pop();
            for(int i=0;i<n;i++){
                int gc=__gcd(p,a[i]);
                if(dp[gc]==1e9){
                    dp[gc]=1+dp[p];
                    q.push(gc);
                }
            }
        }
        cout<<n-1+dp[gcd]<<endl;
    }
}

