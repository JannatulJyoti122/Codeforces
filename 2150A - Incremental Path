#include<bits/stdc++.h>
using namespace std;
using ll =long long int;

int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll n,m;
        cin>>n>>m;
        string s;
        cin>>s;
        getchar();
        set<ll>st;
        for(ll i=0;i<m;i++){
            ll x;
            cin>>x;
            st.insert(x);
        }
        ll x=1;
        for(ll i=0;i<n;i++){
            x++;
            if(s[i]=='B'){
                while(st.find(x)!=st.end()) x++;
            }
            st.insert(x);
            if(s[i]=='B'){
                while(st.find(x)!=st.end()) x++;
            }

        }
        cout<<st.size()<<endl;
        for(auto it:st) cout<<it<<" "; cout<<endl;
    }
}
