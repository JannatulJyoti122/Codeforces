#include<bits/stdc++.h>
using namespace std;
using ll =long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll w,h,a,b;
        cin>>w>>h>>a>>b;
        ll x1,y1,x2,y2;
        cin>>x1>>y1>>x2>>y2;
        if(x1==x2){
            if(abs(y1-y2)%b==0) cout<<"YES\n";
            else cout<<"NO\n";
            continue;
        }
        if(y1==y2){
            if(abs(x1-x2)%a==0) cout<<"YES\n";
            else cout<<"NO\n";
            continue;
        }
        if(abs(x1-x2)%a==0||abs(y1-y2)%b==0) cout<<"YES\n";
        else cout<<"NO\n";
    }
}
