#include<bits/stdc++.h>
using namespace std;
using ll =long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll n,m;
        cin>>n>>m;
        ll a[m];
        for(ll i=0;i<m;i++) cin>>a[i];
        bool f=true;
        for(ll i=0;i<m-1;i++){
            if(a[i]+1!=a[i+1]){
                f=false;
                break;
            }
        }
        if(f==false){
            cout<<1<<endl;
            continue;
        }
        cout<<n-a[m-1]+1<<endl;
    }
}
