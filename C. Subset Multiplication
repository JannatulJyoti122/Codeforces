#include<bits/stdc++.h>
using namespace std;
using ll =long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll n;
        cin>>n;
        ll a[n];
        for(ll i=0;i<n;i++) cin>>a[i];
        ll gcd1=0;
        ll lcm=1;
        for(ll i=0;i<n-1;i++){
            ll gcd=__gcd(a[i],a[i+1]);
            ll x=a[i]/gcd;
            lcm=(lcm*x)/(__gcd(lcm,x));
            //cout<<gcd<<" -- ";
        }
        cout<<lcm<<endl;
    }
}
