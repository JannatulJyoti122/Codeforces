#include<bits/stdc++.h>
#include <ext/pb_ds/assoc_container.hpp>
using namespace __gnu_pbds;
using namespace std;
typedef tree<int, null_type, less<int>, rb_tree_tag,
            tree_order_statistics_node_update>
    ordered_set;
#define ll  long long int
int main(){
    ll tt;
    tt=1;
    cin>>tt;
    while(tt--){
        ll n,m,k;
        cin>>n>>m>>k;
        vector<ll>v(n);
        for(ll i=0;i<m;i++){
            v[i]=m-i;
        }
        for(ll i=m;i<n;i++) v[i]=i+1;
        for(ll i=n-1;i>=0;i--) cout<<v[i]<< " "; cout<<endl;

    }
}
