#include<bits/stdc++.h>
using namespace std;
using ll =long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll n,k;
        cin>>n>>k;
        string s;
        cin>>s;
        ll moves=n/2;
        moves++;
        if(moves<=k) cout<<"Alice\n";
        else {
            ll one=0;
            for(ll i=0;i<n;i++){
                if(s[i]=='1') one++;
            }
            //cout<<one<<endl;
            if(one<=k) cout<<"Alice\n";
            else cout<<"Bob\n";
        }
    }
}
