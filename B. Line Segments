#include<bits/stdc++.h>
using namespace std;
using ll =long long int;
int main(){
    int tt;
    cin>>tt;
    while(tt--){
        ll n;
        cin>>n;
        ll a,b,c,d;
        cin>>a>>b>>c>>d;
        double dis=((a-c)*(a-c))+((b-d)*(b-d))*1.0;
        dis=sqrt(dis);
        ll aa[n];
        double s=0;
        for(ll i=0;i<n;i++){
            cin>>aa[i];
            s+=aa[i];
        }
        s*=1.0;
        s+=dis;
        s/=2;
        bool f=true;
        for(ll i=0;i<n;i++){
            if(aa[i]>s){
                f=false;
                break;
            }
        }
        if(dis>s) f=false;
        if(f) cout<<"YES\n";
        else cout<<"NO\n";
    }
}
