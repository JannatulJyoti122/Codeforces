#include <bits/stdc++.h>
using namespace std;
using ll=long long;
int main() {
    int tt;
    cin>>tt;
    while(tt--){
        string s;
        cin>>s;
        int n=s.size();
        int ans=-1;
        for(int i=0;i<n-1;i++){
            if(s[i]==s[i+1]){
                ans=i+1;
                break;
            }
        }
        if(ans==-1){
            cout<<n<<endl;
            continue;
        }
        cout<<1<<endl;
    }
}
